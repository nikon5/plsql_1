<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane fx:controller="com.korczak.plsql1.controller.ButtonController" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="1000.0"
      xmlns="http://javafx.com/javafx/8 " xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ListView fx:id="listOfTablesNames" onMouseClicked="#handleMouseClick" layoutX="7.0" layoutY="6.0"
                  prefHeight="319.0" prefWidth="200.0"/>

        <Button fx:id="getTableNamesButton" onAction="#onGetTableNamesAction" layoutX="6.0" layoutY="333.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="200.0" text="Get names"/>

        <TextArea fx:id="countText" layoutX="215.0" layoutY="30.0" prefHeight="15.0" prefWidth="375.0"/>

        <Button fx:id="saveToFileButton" onAction="#onTableDescriptionSaveAction" layoutX="220.0" layoutY="333.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="375.0"
                text="Save to file"/>

        <TableView fx:id="descTable" layoutX="215.0" layoutY="85.0" prefHeight="242.0" prefWidth="376.0"/>


        <Label layoutX="211.0" layoutY="14.0" text="Rowcount:"/>
        <Label layoutX="215.0" layoutY="70.0" text="Description:"/>
        <Button fx:id="inputNewRows" onAction="#onInsertRows" layoutX="7.0" layoutY="361.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Insert rows" />
        <TextField fx:id="howManyRows" layoutX="220.0" layoutY="361.0" prefHeight="25.0" prefWidth="375.0" />
    </children>
</Pane>